            <assign variable="media" expression="(janeiro+fevereiro+marco+abril)/4"/>
            <output expression="&quot;O seu ganho M&#233;dio &#233;&quot;" newline="True"/>
            <output expression="media" newline="True"/>
            <if expression="media&gt;=5000">
                <then>
                    <output expression="&quot;Par&#225;bens voc&#234; receber&#225; um abono de 10%&quot;" newline="True"/>
                </then>
                <else>
