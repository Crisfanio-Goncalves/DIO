    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="janeiro, fevereiro, marco, abril, media" type="Real" array="False" size=""/>
            <output expression="&quot;Digite o valor ganho de Janeiro&quot;" newline="True"/>
            <input variable="janeiro"/>
            <output expression="&quot;Digite o valor ganho de Fevereiro&quot;" newline="True"/>
            <input variable="fevereiro"/>
            <output expression="&quot;Digite o valor ganho de Mar&#231;o&quot;" newline="True"/>
